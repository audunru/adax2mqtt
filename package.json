    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "msw": "^2.10.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^2.1.9"
  },
  "dependencies": {
